{
	"version": "2.0.0",
	"tasks": [
        {
            "type": "cargo",
            "subcommand": "build",
            "problemMatcher": [
                "$rustc"
            ],
            "group": "build",
            "label": "Rust: cargo build"
        },
        {
            "type": "cargo",
            "subcommand": "run",
            "problemMatcher": [
                "$rustc"
            ],
            "label": "Rust: cargo run - ???",
            "group": "build"
        },
        {
            "type": "cargo",
            "subcommand": "test",
            "problemMatcher": [
                "$rustc"
            ],
            "group": "build",
            "label": "Rust: cargo test"
        },
        {
            "type": "shell",
            "command": "cargo run --bin import-osm-data pubfiles/tiles/osmdata.xml",
            "label": "Rust: cargo run - import-osm-data",
            "group": "build"
        },
        {
            "type": "shell",
            "command": "cargo run --bin import-game-data sample_game_data.xml",
            "label": "Rust: cargo run - import-game-data",
            "group": "build"
        },
        {
            "type": "shell",
            "command": "cargo run --bin tmou-mapa-backend",
            "label": "Rust: cargo run - tmou-mapa-backend",
            "group": "build",
            "problemMatcher": []
        },
        {
            "type": "shell",
            "command": "cargo test --no-run",
            "label": "Rust: cargo build tests",
            "group": "build"
        },
        {
            "type": "shell",
            "command": "cargo test -- --skip admin_is_protected_by_password",
            "label": "Rust: cargo test without failing admin",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        }
    ]
}